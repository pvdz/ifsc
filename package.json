{
  "name": "gatsby-starter-hello-world",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "split:develop": "mkdirp ./src/data/develop && ./node_modules/.bin/csvsplit -c 1 -o ./src/data/develop ./src/data/original/develop.csv",
    "split:build": "mkdirp src/data/build && ./node_modules/.bin/csvsplit -c 1 -o ./src/data/build ./src/data/original/build.csv",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "dependencies": {
    "bulma": "^0.8.0",
    "gatsby": "^2.18.25",
    "gatsby-plugin-sass": "^2.1.26",
    "gatsby-plugin-schema-snapshot": "^1.0.0",
    "gatsby-plugin-typescript": "^2.1.23",
    "gatsby-source-filesystem": "^2.1.43",
    "gatsby-transformer-json": "^2.2.25",
    "lodash": "^4.17.15",
    "node-sass": "^4.13.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "slugify": "^1.3.6"
  },
  "devDependencies": {
    "prettier": "^1.19.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
